<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tf6uk"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" 
     android:layout_gravity="center">

    <TextView
        android:id="@+id/testView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="130dp"
        android:gravity="center"
        android:text="textfield 1-2"
        android:textColor="#FDD017"
        android:textSize="14sp"
        android:textStyle="bold"
        android:typeface="serif" />
     
     <TextView
         android:id="@+id/testView01"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_below="@+id/testView02"
         android:layout_centerHorizontal="true"
         android:gravity="center"
         android:text="textfield 1-2"
         android:textColor="#FDD017"
         android:textSize="14sp"
         android:textStyle="bold"
         android:typeface="serif" />
     
     <TextView
         android:id="@+id/testIqamahtimings"
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
         android:layout_below="@+id/testView01"
         android:layout_marginTop="12dp"
         android:gravity="center"
         android:maxLines="2"
        
         android:textColor="#FFD732"
         android:textSize="17sp"
         android:textStyle="bold" />

    <TableLayout
        android:id="@+id/TableLayout01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/testView02"
        android:layout_centerHorizontal="true" >

        <TableRow
            android:id="@+id/TableRow02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/testView03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="60dp"
                android:gravity="left"
                android:padding="10dp"
                android:text="textfield 2-1"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

            <TextView
                android:id="@+id/testView04"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="60dp"
                android:gravity="right"
                android:padding="10dp"
                android:text="textfield 2-2"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

        </TableRow>

        <TableRow
            android:id="@+id/TableRow03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/testView05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:padding="10dp"
                android:text="textfield 3-1"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

            <TextView
                android:id="@+id/testView06"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:padding="10dp"
                android:text="textfield 3-2"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

        </TableRow>

        <TableRow
            android:id="@+id/TableRow04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/testView07"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:padding="10dp"
                android:text="textfield 4-1"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

            <TextView
                android:id="@+id/testView08"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:padding="10dp"
                android:text="textfield 4-2"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

        </TableRow>

        <TableRow
            android:id="@+id/TableRow05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/testView09"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:padding="10dp"
                android:text="textfield 5-1"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

            <TextView
                android:id="@+id/testView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:layout_marginLeft="30px"
                android:padding="10dp"
                android:text="textfield 5-2"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

        </TableRow>

        <TableRow
            android:id="@+id/TableRow06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/testView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:padding="10dp"
                android:text="textfield 6-1"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

            <TextView
                android:id="@+id/testView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
        		android:layout_marginLeft="14px"
                android:padding="10dp"
                android:text="textfield 6-2"
                android:textColor="#FDD017"
                android:textSize="20sp"
                android:typeface="serif" />

        </TableRow>
    </TableLayout>

    <TextView
        android:id="@+id/testaddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TableLayout01"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:textColor="#FDD017"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="serif" />

    <TextView
        android:id="@+id/testphone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/testaddress"
        android:layout_centerHorizontal="true"
        android:gravity="center"
      
        android:textColor="#FDD017"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="serif" />

    <TextView
        android:id="@+id/visittestwebsite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/testphone"
        android:layout_centerHorizontal="true"

        android:gravity="center"
        android:onClick="visittestWebsite"
        android:clickable="true"
        android:text="@string/visitwebsite"
        android:textColor="#FDD017"
        android:textColorLink="#FDD017"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="serif" />


</RelativeLayout>